<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- State ketika item menu ini terpilih/aktif (checked) -->
    <!-- Android akan menampilkan versi ikon yang aktif (kuning) -->
    <item android:drawable="@drawable/ic_bottom_nav_profile_active" android:state_checked="true" />

    <!-- State default (ketika tidak terpilih) -->
    <!-- Android akan menampilkan versi ikon yang tidak aktif (abu-abu) -->
    <item android:drawable="@drawable/ic_bottom_nav_profile_inactive" />

</selector>
